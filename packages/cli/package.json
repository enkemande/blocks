{
  "name": "nke-blocks",
  "version": "1.0.1",
  "description": "Build, Publish and Download react components",
  "main": "index.js",
  "type": "module",
  "author": {
    "name": "yoinch",
    "url": "https://twitter.com/yoinch"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "nke-blocks": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enkemande/blocks-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "node dist/index.js",
    "start": "node dist/index.js",
    "pub:beta": "pnpm run build && npm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm run build && npm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm run build && npm publish --access public"
  },
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "radix-ui",
    "shadcn",
    "shadcn-blocks",
    "cli",
    "tailwindcss"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.11",
    "@types/prompts": "^2.4.9",
    "tsup": "^6.6.3",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "adm-zip": "^0.5.12",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  }
}
